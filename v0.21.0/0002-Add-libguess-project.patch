index 000000000..9396e1509
--- /dev/null
+++ b/build_msvc/libguess/libguessmodule.cu
@@ -0,0 +1,158 @@
+#define PY_SSIZE_T_CLEAN
+#include <Python.h>
+#include <cuda_runtime.h>
+#include "guess.h"
+#include "print.h"
+
+PyDoc_STRVAR(libguess_runGuess_doc, "runGuess(device: int, blockHeader: bytes, target: bytes, maxTries: int = 1) -> Tuple[result, error]\
+\
+Run GPU guess function.");
+PyDoc_STRVAR(libguess_listDevices_doc, "libguess_listDevices() -> Tuple[result, error]\
+\
+List compute devives.");
+
+static PyObject* PyGuessResult(const GuessResult& guessResult)
+{
+    PyObject* pResult = Py_BuildValue("(OIIy#)",
+        guessResult.bGuessed ? Py_True : Py_False,
+        guessResult.bestNonce,
+        guessResult.bestTime,
+        (const char*)guessResult.bestHash, sizeof(guessResult.bestHash));
+    return Py_BuildValue("(OO)", pResult, Py_None);
+}
+
+static PyObject* PyFailureResult(int errorCode)
+{
+    PyObject* pError = Py_BuildValue("(is)", errorCode, GetErrorString(errorCode));
+    return Py_BuildValue("(OO)", Py_None, pError);
+}
+
+PyObject* libguess_listDevices()
+{
+    DeviceProp* devProps;
+    int devCount;
+    int errorCode;
+    
+    errorCode = ListDevices(&devProps, &devCount);
+    if (errorCode)
+        return PyFailureResult(errorCode);
+
+    if (devCount == 0)
+        return Py_BuildValue("([], O)", Py_None);
+
+    PyObject* pDeviceList = PyList_New(devCount);
+    for (int device = 0; device < devCount; device++)
+    {
+        PyObject* pDeviceProp = Py_BuildValue("{s:i,s:i}",
+            "device", device,
+            "multiprocessors", devProps[device].multiprocessors
+        );
+        PyList_SetItem(pDeviceList, device, pDeviceProp);
+    }
+    delete devProps;
+   
+    return Py_BuildValue("(OO)", pDeviceList, Py_None);
+}
+
+PyObject* libguess_runGuess(PyObject* self, PyObject* args, PyObject* kwargs) {
+    /* Shared references that do not need Py_DECREF before returning. */
+    int device;
+    Py_buffer blockHeader, target;
+    int maxTries = 1;
+
+    /* Parse positional and keyword arguments */
+    static const char* keywords[] = { "device", "blockHeader", "target", "maxTries", NULL };
+    if (!PyArg_ParseTupleAndKeywords(args, kwargs, "iy*y*|i", (char**)keywords, &device, &blockHeader, &target, &maxTries)) {
+        return NULL;
+    }
+
+    if (blockHeader.len != HEADER_SIZE)
+    {
+        PyErr_SetString(PyExc_ValueError, "incorrect blockHeader size");
+        return NULL;    /* return NULL indicates error */
+    }
+
+    if (target.len != HASH_SIZE)
+    {
+        PyErr_SetString(PyExc_ValueError, "incorrect target size");
+        return NULL;    /* return NULL indicates error */
+    }
+
+    if (maxTries < 1)
+    {
+        PyErr_SetString(PyExc_ValueError, "zero or negative maxTries");
+        return NULL;    /* return NULL indicates error */
+    }
+
+    int errorCode;
+
+    errorCode = SelectDevice(device);
+    if (errorCode)
+        return PyFailureResult(errorCode);
+
+    LogPrintf("runGuess(device=%d, blockHeader=%s, target=%s, maxTries=%d)\n", device,
+        SPrintBuf((unsigned char*)blockHeader.buf, blockHeader.len).c_str(),
+        SPrintBuf((unsigned char*)target.buf, target.len).c_str(),
+        maxTries);
+
+    GuessResult guessResult;
+
+    Py_BEGIN_ALLOW_THREADS
+    errorCode = RunGuess((unsigned char*)blockHeader.buf, (unsigned char*)target.buf, maxTries, guessResult);
+    Py_END_ALLOW_THREADS
+
+    if (errorCode)
+        return PyFailureResult(errorCode);
+
+    return PyGuessResult(guessResult);
+}
+
+/*
+ * List of functions to add to libguess in exec_libguess().
+ */
+static PyMethodDef libguess_functions[] = {
+    { "listDevices", (PyCFunction)libguess_listDevices, METH_NOARGS, libguess_listDevices_doc },
+    { "runGuess", (PyCFunction)libguess_runGuess, METH_VARARGS | METH_KEYWORDS, libguess_runGuess_doc },
+    { NULL, NULL, 0, NULL } /* marks end of array */
+};
+
+/*
+ * Initialize libguess. May be called multiple times, so avoid
+ * using static state.
+ */
+int exec_libguess(PyObject* module) {
+    PyModule_AddFunctions(module, libguess_functions);
+
+    PyModule_AddStringConstant(module, "__author__", "Marsel");
+    PyModule_AddStringConstant(module, "__version__", "1.0.0");
+    PyModule_AddIntConstant(module, "year", 2021);
+
+    return 0; /* success */
+}
+
+/*
+ * Documentation for libguess.
+ */
+PyDoc_STRVAR(libguess_doc, "The libguess module");
+
+
+static PyModuleDef_Slot libguess_slots[] = {
+    { Py_mod_exec, exec_libguess },
+    { 0, NULL }
+};
+
+static PyModuleDef libguess_def = {
+    PyModuleDef_HEAD_INIT,
+    "libguess",
+    libguess_doc,
+    0,              /* m_size */
+    NULL,           /* m_methods */
+    libguess_slots,
+    NULL,           /* m_traverse */
+    NULL,           /* m_clear */
+    NULL,           /* m_free */
+};
+
+PyMODINIT_FUNC PyInit_libguess() {
+    return PyModuleDef_Init(&libguess_def);
+}
