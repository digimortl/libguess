+#include "pch.h"
+
+#include <cuda_runtime.h>
+
+#include "guess.h"
+#include "guess_kernel.cuh"
+#include "print.h"
+
+LIB_API const char* GetErrorString(const int errorCode)
+{
+    return cudaGetErrorString(cudaError_t(errorCode));
+}
+
+LIB_API int SelectDevice(int device)
+{
+    cudaError_t cudaStatus;
+
+    cudaStatus = cudaSetDevice(device);
+    if (cudaStatus != cudaSuccess)
+        return cudaStatus;
+
+    return cudaSetDeviceFlags(cudaDeviceScheduleBlockingSync);
+}
+
+LIB_API int ListDevices(DeviceProp* deviceProps[], int* deviceCount)
+{
+    cudaError_t cudaStatus;
+
+    cudaStatus = cudaGetDeviceCount(deviceCount);
+    if (cudaStatus != cudaSuccess)
+        return cudaStatus;
+
+    if (*deviceCount == 0)
+    {
+        *deviceProps = nullptr;
+        return cudaStatus;
+    }
+
+    *deviceProps = new DeviceProp[*deviceCount];
+
+    for (int device = 0; device < *deviceCount; device++)
+    {
+        (*deviceProps)[device].device = device;
+        (*deviceProps)[device].multiprocessors = 0;
+
+        cudaDeviceProp prop;
+        cudaStatus = cudaGetDeviceProperties(&prop, device);
+        if (cudaStatus != cudaSuccess) {
+            fprintf(stderr, "%s\n", cudaGetErrorString(cudaStatus));
+            continue;
+        }
+
+        (*deviceProps)[device].multiprocessors = prop.multiProcessorCount;
+    }
+
+    return cudaSuccess;
+}
+
+LIB_API int RunGuess(const unsigned char blockHeader[HEADER_SIZE], const unsigned char target[HASH_SIZE], const int& maxTries, GuessResult& guessResult)
+{
+    GuessArgs* d_guessArgs = nullptr;
+    GuessArgs h_guessArgs;
+    cudaError_t cudaStatus;
+    dim3 grid(BlocksPerGrid * maxTries), block(ThreadsPerBlock);
+
+    cudaStatus = cudaMalloc((void**)&d_guessArgs, sizeof(GuessArgs));
+    if (cudaStatus != cudaSuccess) {
+        fprintf(stderr, "cudaMalloc failed!");
+        goto Error;
+    }
+
+    PrepareGuess(&h_guessArgs, blockHeader, target);
+
+    cudaStatus = cudaMemcpy(d_guessArgs, &h_guessArgs, sizeof(GuessArgs), cudaMemcpyHostToDevice);
+    if (cudaStatus != cudaSuccess) {
+        fprintf(stderr, "cudaMemcpy failed!");
+        goto Error;
+    }
+
+    LogPrintf("<<<Grid(0x%lx,0x%lx,0x%lx),Block(0x%lx,0x%lx,0x%lx)>>>\n", grid.x, grid.y, grid.z, block.x, block.y, block.z);
+
+    Guess << <grid, block >> > (d_guessArgs);
+
+    cudaStatus = cudaGetLastError();
+    if (cudaStatus != cudaSuccess) {
+        fprintf(stderr, "Guess kernel launch failed: %s\n", cudaGetErrorString(cudaStatus));
+        goto Error;
+    }
+
+    cudaStatus = cudaDeviceSynchronize();
+    if (cudaStatus != cudaSuccess) {
+        fprintf(stderr, "cudaDeviceSynchronize returned error code %d after launching addKernel!\n", cudaStatus);
+        goto Error;
+    }
+
+    cudaStatus = cudaMemcpy(&h_guessArgs, d_guessArgs, sizeof(GuessArgs), cudaMemcpyDeviceToHost);
+    if (cudaStatus != cudaSuccess) {
+        fprintf(stderr, "cudaMemcpy failed!");
+        goto Error;
+    }
+
+    if (h_guessArgs.bestTimeAndNonce > 0)
+        LogPrintf("guessed times: %d\n", h_guessArgs.nGuessed);
+    LogPrintf("best hash    : 0x%s\n", SPrintHash((unsigned char*)h_guessArgs.bestHash).c_str());
+    LogPrintf("target       : 0x%s\n\n", SPrintHash((unsigned char*)h_guessArgs.target).c_str());
+
+    guessResult.bGuessed = h_guessArgs.bestTimeAndNonce > 0;
+    guessResult.bestTime = h_guessArgs.bestTimeAndNonce >> 32;
+    guessResult.bestNonce = h_guessArgs.bestTimeAndNonce & 0xfffffffful;
+    memcpy(guessResult.bestHash, h_guessArgs.bestHash, HASH_SIZE);
+
+Error:
+    cudaFree(d_guessArgs);
+
+    return cudaStatus;
+}
